<Window x:Class="Bookstore_App.ViewOrderDetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bookstore_App"
        mc:Ignorable="d"
        Title="ViewOrderDetails" Height="450" Width="800">
    <Grid Background="#FF41CA69">
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,0,199" />
        <Button Content="Back" HorizontalAlignment="Center" Margin="0,321,0,0" VerticalAlignment="Top" FontSize="20" Background="#FFB53636" Click="Button_Click_1" RenderTransformOrigin="0.36,2.926"/>
    </Grid>
</Window>
